#%Module1.0#####################################################################
##
## tabix modulefile
##

source $env(MODULE_INCLUDE)/functions.tcl
getCluster

set components [ file split [ module-info name ] ]
set version [ lindex $components 1 ]

set     modroot          /proj/a2010002/nobackup/sw/mf/bioinfo-tools/tabix/tabix-0.2.5

proc ModulesHelp { } {
        global version modroot

        puts stderr "\ttabix - use tabix $version"
        puts stderr "\n\tVersion $version\n"
}

module-whatis   "Loads tabix environment."

# Only one version at a time
conflict tabix

#Log loading to syslog
logToSyslog

# Directories for the program:
#
prepend-path    PATH            $modroot

# Environments for the program:
#
setenv               TABIX_HOME      $modroot
